{
  "name": "bartter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate-routes && tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && stylelint **/*.{css,scss,sass} --fix",
    "preview": "vite preview",
    "generate-routes": "tsr generate",
    "watch-routes": "tsr watch",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.51.11",
    "@tanstack/react-router": "^1.45.8",
    "axios": "^1.7.2",
    "browser-image-compression": "^2.0.2",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "immer": "^10.1.1",
    "localforage": "^1.10.0",
    "react": "^18.3.1",
    "react-calendar": "^5.0.0",
    "react-datepicker": "^7.3.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-lottie-player": "^2.1.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/eslint-parser": "^7.24.8",
    "@svgr/rollup": "^8.1.0",
    "@tanstack/router-cli": "^1.45.8",
    "@tanstack/router-devtools": "^1.45.8",
    "@tanstack/router-plugin": "^1.45.8",
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-slick": "^0.23.13",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.7",
    "msw": "^2.3.4",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "stylelint": "^16.7.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vite-plugin-svgr": "^4.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,sass}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
